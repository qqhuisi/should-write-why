<html>
<head>
<title>变量的类型</title>
<script>
var  a=true;  //boolean
var  b=function(){}; //function
var  c;//undefined
var  d=12;//number
var  e=null;  //object
var  f={}; //object
var  g='';//string
var  h=new Date().getDay();  //number

console.log(typeof  a);
console.log(typeof  b);
console.log(typeof  c);
console.log(typeof  d);
console.log(typeof  e);
console.log(typeof  f);
console.log(typeof  g);
console.log(h);
</script>

</head>
<body>
</body>
</html>
